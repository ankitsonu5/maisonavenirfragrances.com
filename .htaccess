# Redirect non-www to www
RewriteCond %{HTTP_HOST} ^maisonavenirfragrances\.com [NC]
RewriteRule ^(.*)$ http://www.maisonavenirfragrances.com/$1 [L,R=301]

<IfModule mod_rewrite.c>
    RewriteEngine On
    # Direct all traffic to the public directory
    RewriteRule ^(.*)$ public/$1 [L]
</IfModule>

# PHP handler configuration for cPanel (do not edit)
<IfModule mime_module>
    AddHandler application/x-httpd-ea-php82 .php .php8 .phtml
</IfModule>
